<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tt">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TT">
        <activity
            android:name=".Thursday"
            android:label="@string/title_activity_thursday"
            android:theme="@style/Theme.TT.NoActionBar"></activity>
        <activity android:name=".Wednesday" />
        <activity android:name=".Tuesday" />
        <activity android:name=".Monday" />
        <activity android:name=".Friday" />
        <activity
            android:name=".tuesday"
            android:label="@string/title_activity_tuesday"
            android:theme="@style/Theme.TT.NoActionBar" />
        <activity
            android:name=".monday"
            android:label="@string/title_activity_monday"
            android:theme="@style/Theme.TT.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>